fmu:FMUHull a fmu:FMU, sosa:FeatureOfInterest, ssn:System ;
  rdfs:label "Hull_FMU" ;
  fmu:fmuType "ShipHull" ;
  fmu:FMUComponentID "HULL_001" ;
  fmu:hasVariable fmu:HullSpeed .

fmu:HullSpeed a sosa:ObservableProperty ;
  ssn:isPropertyOf fmu:FMUHull ;
  fmu:hasFMUVariableName "V_knots" ;
  fmu:hasDataType xsd:double .

