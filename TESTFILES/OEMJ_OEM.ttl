@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn:  <http://www.w3.org/ns/ssn/> .
@prefix fmu:  <http://example.com/fmu#> .
@prefix qudt: <http://qudt.org/2.1/schema/qudt#> .
@prefix unit: <http://qudt.org/2.1/vocab/unit#> .
@prefix oemj: <http://example.com/oemj#> .
oemj:System001 a sosa:FeatureOfInterest, ssn:System ;
  rdfs:label "Marinus Power Demo System" ;
  fmu:FMUComponentID "OEMJ_SYS_001" .

oemj:Var01 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "Qe_limit_kNm" ;
  qudt:unit unit:KiloN-M ;
  rdfs:comment "Prime mover interface variable from engine datasheet at MCR." .
oemj:Var01_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var01 ;
  sosa:hasSimpleResult "722.93"^^xsd:decimal .

oemj:Var02 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "startAirCap_Nm3" ;
  rdfs:comment "Starting air system capacity/time from machinery arrangement." .
oemj:Var02_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var02 ;
  sosa:hasSimpleResult "1.96"^^xsd:decimal .

oemj:Var03 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "CW_ChestVolume_m3" ;
  qudt:unit unit:M3 ;
  rdfs:comment "Cooling water inlet chest sizing / performance indicator." .
oemj:Var03_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var03 ;
  sosa:hasSimpleResult "8.69"^^xsd:decimal .

oemj:Var04 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "DesignThrust_kN" ;
  qudt:unit unit:KiloN ;
  rdfs:comment "Design response variable for shaft-line checks per project spec." .
oemj:Var04_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var04 ;
  sosa:hasSimpleResult "606.2"^^xsd:decimal .

oemj:Var05 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "n_nominal_rpm" ;
  qudt:unit unit:REV-PER-MIN ;
  rdfs:comment "Propeller geometry/load parameter for design/performance checks." .
oemj:Var05_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var05 ;
  sosa:hasSimpleResult "616.0"^^xsd:decimal .

oemj:Var06 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "Q_max_ice_kNm" ;
  qudt:unit unit:KiloN-M ;
  rdfs:comment "Propeller geometry/load parameter for design/performance checks." .
oemj:Var06_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var06 ;
  sosa:hasSimpleResult "132.29"^^xsd:decimal .

oemj:Var07 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "natFreq_Hz" ;
  qudt:unit unit:HZ ;
  rdfs:comment "Thruster body/load/vibration parameter for class verification." .
oemj:Var07_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var07 ;
  sosa:hasSimpleResult "8.88"^^xsd:decimal .

oemj:Var08 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemj:System001 ;
  fmu:hasFMUVariableName "resWithin20pct" ;
  rdfs:comment "Design response variable for shaft-line checks per project spec." .
oemj:Var08_Obs a sosa:Observation ;
  sosa:observedProperty oemj:Var08 ;
  sosa:hasSimpleResult "2.94"^^xsd:decimal .

