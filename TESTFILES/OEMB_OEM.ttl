@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn:  <http://www.w3.org/ns/ssn/> .
@prefix fmu:  <http://example.com/fmu#> .
@prefix qudt: <http://qudt.org/2.1/schema/qudt#> .
@prefix unit: <http://qudt.org/2.1/vocab/unit#> .
@prefix oemb: <http://example.com/oemb#> .
oemb:System001 a sosa:FeatureOfInterest, ssn:System ;
  rdfs:label "BalticDrive Systems Demo System" ;
  fmu:FMUComponentID "OEMB_SYS_001" .

oemb:Var01 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "IceImpact_kN" ;
  qudt:unit unit:KiloN ;
  rdfs:comment "Thruster body/load/vibration parameter for class verification." .
oemb:Var01_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var01 ;
  sosa:hasSimpleResult "790.9"^^xsd:decimal .

oemb:Var02 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "n_MCR_rpm" ;
  qudt:unit unit:REV-PER-MIN ;
  rdfs:comment "Propeller geometry/load parameter for design/performance checks." .
oemb:Var02_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var02 ;
  sosa:hasSimpleResult "771.9"^^xsd:decimal .

oemb:Var03 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "Dia_m" ;
  qudt:unit unit:M ;
  rdfs:comment "Propeller geometry/load parameter for design/performance checks." .
oemb:Var03_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var03 ;
  sosa:hasSimpleResult "7.303"^^xsd:decimal .

oemb:Var04 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "screenOpenAreaRatio" ;
  rdfs:comment "Cooling water inlet chest sizing / performance indicator." .
oemb:Var04_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var04 ;
  sosa:hasSimpleResult "2.35"^^xsd:decimal .

oemb:Var05 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "respTorque_kNm" ;
  qudt:unit unit:KiloN-M ;
  rdfs:comment "Design response variable for shaft-line checks per project spec." .
oemb:Var05_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var05 ;
  sosa:hasSimpleResult "778.76"^^xsd:decimal .

oemb:Var06 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "bladeOrderResonanceNearMax" ;
  rdfs:comment "Design response variable for shaft-line checks per project spec." .
oemb:Var06_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var06 ;
  sosa:hasSimpleResult "0.49"^^xsd:decimal .

oemb:Var07 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "airRecCap_Nm3" ;
  rdfs:comment "Starting air system capacity/time from machinery arrangement." .
oemb:Var07_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var07 ;
  sosa:hasSimpleResult "4.16"^^xsd:decimal .

oemb:Var08 a sosa:ObservableProperty ;
  ssn:isPropertyOf oemb:System001 ;
  fmu:hasFMUVariableName "reverseRequired" ;
  rdfs:comment "Prime mover interface variable from engine datasheet at MCR." .
oemb:Var08_Obs a sosa:Observation ;
  sosa:observedProperty oemb:Var08 ;
  sosa:hasSimpleResult "2.93"^^xsd:decimal .

