
@prefix : <http://example.com/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn:  <http://www.w3.org/ns/ssn/> .
@prefix fmu:  <http://example.com/fmu#> .
@prefix qudt: <http://qudt.org/2.1/schema/qudt#> .
@prefix unit: <http://qudt.org/2.1/vocab/unit#> .

fmu:FMUEngine_Test1 a fmu:FMU, sosa:FeatureOfInterest, ssn:System ;
  rdfs:label "Engine_FMU_Test1" ;
  fmu:fmuType "DieselEngine" ;
  fmu:FMUComponentID "ENGINE_TEST_001" ;
  fmu:hasVariable fmu:Engine_Power .

fmu:Engine_Power a sosa:ObservableProperty ;
  ssn:isPropertyOf fmu:FMUEngine_Test1 ;
  fmu:hasFMUVariableName "Engine_Power" ;
  fmu:hasDataType xsd:double ;
  qudt:unit unit:KiloW .

fmu:Engine_Power_Value a sosa:Observation ;
  sosa:observedProperty fmu:Engine_Power ;
  sosa:hasSimpleResult "3000.0"^^xsd:double .
